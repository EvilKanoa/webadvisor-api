{
  "name": "webadvisor-api",
  "version": "0.0.1",
  "description": "Lightweight and simple GraphQL API for UoG, UW, and WLU.",
  "main": "src/server.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -r dotenv/config src/server.js",
    "prod": "NODE_ENV=production node -r dotenv/config src/server.js",
    "test": "jest --passWithNoTests",
    "testing": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvilKanoa/webadvisor-api.git"
  },
  "author": "Kanoa Haley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EvilKanoa/webadvisor-api/issues"
  },
  "homepage": "https://github.com/EvilKanoa/webadvisor-api#readme",
  "prettier": {
    "printWidth": 90,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "cheerio": "1.0.0-rc.2",
    "cors": "2.8.5",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "mongoose": "5.3.10",
    "redis": "2.8.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.5"
  },
  "devDependencies": {
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "supertest": "4.0.2"
  }
}
